#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/vocs/vocs/node_modules/.pnpm/tailwindcss@3.4.13_ts-node@10.9.1_@types+node@22.7.4_typescript@5.2.2_/node_modules/tailwindcss/lib/node_modules:/home/runner/work/vocs/vocs/node_modules/.pnpm/tailwindcss@3.4.13_ts-node@10.9.1_@types+node@22.7.4_typescript@5.2.2_/node_modules/tailwindcss/node_modules:/home/runner/work/vocs/vocs/node_modules/.pnpm/tailwindcss@3.4.13_ts-node@10.9.1_@types+node@22.7.4_typescript@5.2.2_/node_modules:/home/runner/work/vocs/vocs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/vocs/vocs/node_modules/.pnpm/tailwindcss@3.4.13_ts-node@10.9.1_@types+node@22.7.4_typescript@5.2.2_/node_modules/tailwindcss/lib/node_modules:/home/runner/work/vocs/vocs/node_modules/.pnpm/tailwindcss@3.4.13_ts-node@10.9.1_@types+node@22.7.4_typescript@5.2.2_/node_modules/tailwindcss/node_modules:/home/runner/work/vocs/vocs/node_modules/.pnpm/tailwindcss@3.4.13_ts-node@10.9.1_@types+node@22.7.4_typescript@5.2.2_/node_modules:/home/runner/work/vocs/vocs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tailwindcss/lib/cli.js" "$@"
else
  exec node  "$basedir/../tailwindcss/lib/cli.js" "$@"
fi
