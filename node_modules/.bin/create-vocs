#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/work/vocs/vocs/node_modules/.pnpm/create-vocs@1.0.0-alpha.1-main.20240111T192716_7zx332dsc4krcgln5tyrbb3qqy/node_modules/create-vocs/_lib/node_modules:/home/runner/work/vocs/vocs/node_modules/.pnpm/create-vocs@1.0.0-alpha.1-main.20240111T192716_7zx332dsc4krcgln5tyrbb3qqy/node_modules/create-vocs/node_modules:/home/runner/work/vocs/vocs/node_modules/.pnpm/create-vocs@1.0.0-alpha.1-main.20240111T192716_7zx332dsc4krcgln5tyrbb3qqy/node_modules:/home/runner/work/vocs/vocs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/runner/work/vocs/vocs/node_modules/.pnpm/create-vocs@1.0.0-alpha.1-main.20240111T192716_7zx332dsc4krcgln5tyrbb3qqy/node_modules/create-vocs/_lib/node_modules:/home/runner/work/vocs/vocs/node_modules/.pnpm/create-vocs@1.0.0-alpha.1-main.20240111T192716_7zx332dsc4krcgln5tyrbb3qqy/node_modules/create-vocs/node_modules:/home/runner/work/vocs/vocs/node_modules/.pnpm/create-vocs@1.0.0-alpha.1-main.20240111T192716_7zx332dsc4krcgln5tyrbb3qqy/node_modules:/home/runner/work/vocs/vocs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../create-vocs/_lib/bin.js" "$@"
else
  exec node  "$basedir/../create-vocs/_lib/bin.js" "$@"
fi
